
@model MyLogin.Models.Users
@{
    ViewBag.Title = "Login";
}
<p>@Html.ActionLink("Click here to view the public lists", "Index")</p>
<h2>Login</h2>
@Html.ValidationSummary(true)

@using (Html.BeginForm())
{
    @Html.LabelFor(u => u.Email); <br/>
    @Html.TextBoxFor(u => u.Email); <br />
    @Html.LabelFor(u => u.Password); <br />
    @Html.PasswordFor(u =>  u.Password); <br />
    
    <p>
        <button type="submit">Log In</button>
    </p>

}



